{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/privacy.html",
        "destination": "/privacy",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "/apps/**",
        "destination": "/apps/app/index.html"
      },
      {
        "source": "/api/other-apps",
        "function": {
          "functionId": "otherAppsFeed",
          "region": "europe-west1"
        }
      }
    ]
  }
}
